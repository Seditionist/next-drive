{
  "name": "next-drive",
  "version": "1.0.0",
  "description": "google drive clone built in nextjs",
  "main": "build/index.js",
  "scripts": {
    "db:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "db:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "db:down": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "db:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "start": "npm run build && node .",
    "build": "tsc",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Seditionist/next-drive.git"
  },
  "author": "JG^2",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Seditionist/next-drive/issues"
  },
  "homepage": "https://github.com/Seditionist/next-drive#readme",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "class-transformer": "^0.4.0",
    "fastify": "^3.15.1",
    "fastify-autoload": "^3.7.1",
    "fastify-plugin": "^3.0.0",
    "fastify-swagger": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "sanitize-filename": "^1.6.3",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.32"
  }
}
